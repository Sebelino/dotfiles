!URxvt*font:            xft:inconsolata:size=8,xft:monospace:size=8
URxvt*font:            xft:monospace:size=9
URxvt*urlLauncher:     chrome  
URxvt*scrollBar:       false
URxvt*scrollBar_right: false
URxvt*scrollColor:     #000000
URxvt*allow_bold:      true
URxvt*shading:         0
URxvt*fading:          50
URxvt*scrollstyle:     rxvt
URxvt*color4:          RoyalBlue
URxvt*letterSpace:     0

URxvt*scrollTtyOutput:   false
URxvt*scrollWithBuffer:  true
URxvt*scrollTtyKeypress: true
URxvt*background:        rgba:0000/0000/0000/c800

URxvt.depth: 32
URxvt.intensityStyles: false
URxvt.background:      rgba:1111/1111/1111/dddd
URxvt.foreground:      white
URxvt.borderless:      true
URxvt.transparent:     false
URxvt.highlightColor:  #d01018
URxvt.underlineURLs:   True

! Single-line scrolling
URxvt.keysym.Shift-Up: command:\033]720;1\007
URxvt.keysym.Shift-Down: command:\033]721;1\007

URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.iso14755: False
URxvt.perl-ext-common: default,clipboard,keyboard-select,selection-to-clipboard

! CTRL + ALT + Ã¥
!URxvt.keysym.M-0xe5: perl:keyboard-select:activate
URxvt.keysym.M-0xe5: perl:keyboard-select:search

! Savelines
URxvt.saveLines:  20000

! Fixes Inconsolata font difference 2 Jan 2016
Xft.hintstyle:  hintnone

! ! urxvt font setting
! Xft.autohint: 0
! Xft.lcdfilter:  lcddefault
! Xft.hinting: 1
! Xft.antialias: 1
! Xft.rgba: rgb
